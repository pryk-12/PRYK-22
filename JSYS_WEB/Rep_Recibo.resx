  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT dbo.RECIBOS_COBRADOS.ID_RECIBO, 
       dbo.RECIBOS_COBRADOS.ID_FACTURA, 
	   dbo.RECIBOS_COBRADOS.CUOTA_COBRADA, 
	   dbo.RECIBOS_COBRADOS.VALOR_CUOTA, 
       dbo.RECIBOS_COBRADOS.VALOR_PAGADO, 
	   dbo.RECIBOS_COBRADOS.VALOR_PENDIENTE, 
	   dbo.RECIBOS_COBRADOS.FECHA_CREADO, 
	   dbo.RECIBOS_COBRADOS.CREADO_POR AS COBRADOR, 
       dbo.RECIBOS_COBRADOS.CANTIDAD_CUOTAS, 
	   dbo.RECIBOS_COBRADOS.ANULADO_POR, 
	   dbo.RECIBOS_COBRADOS.FECHA_ANULADO, 
	   dbo.RECIBOS_COBRADOS.NOTA_ANULACION, 
       dbo.FACTURAS.REFERENCIA, 
	   dbo.Primera_Letra_Mayuscula(dbo.CLIENTES.NOMBRE) NOMBRE, 
	   dbo.FACTURAS.MONTO_PENDIENTE,
	   CASE dbo.RECIBOS_COBRADOS.ESTADO
	   WHEN 'V' THEN 'Vigente'
	   WHEN 'A' THEN 'Anulado'
	   END ESTADO,
	  CASE dbo.FACTURAS.ID_ESTADO 
	  WHEN 5 THEN 'Si'
	  END ENTREGADO,
	  CASE dbo.FACTURAS.ID_ESTADO 
	  WHEN 5 THEN dbo.FACTURAS.FECHA_CAMBIO_ESTADO
	  END FECHA_ENTREGA,
	   (select SUM(MONTO_PENDIENTE) FROM DETALLES_CUOTAS WHERE FECHA &lt; FORMAT(GETDATE(),'yyyy-MM-dd') AND RE_CALCULADO='NO' AND MONTO_PENDIENTE &gt;0 AND ID_FACTURA = dbo.RECIBOS_COBRADOS.ID_FACTURA) AS MONTO_PENDIENTE_ATRASO,
	   (select STUFF((SELECT CAST(',' AS varchar(MAX)) + CAST(NUMERO_CUOTA AS varchar(3)) FROM DETALLES_CUOTAS WHERE FECHA &lt; FORMAT(GETDATE(),'yyyy-MM-dd') AND RE_CALCULADO='NO' AND MONTO_PENDIENTE &gt;0 AND ID_FACTURA = dbo.RECIBOS_COBRADOS.ID_FACTURA ORDER BY NUMERO_CUOTA FOR XML PATH('')), 1, 1, '')) as CUOTAS_ATRASADAS,
	    dbo.Primera_Letra_Mayuscula((dbo.NUMERO_A_LETRAS(dbo.RECIBOS_COBRADOS.VALOR_PAGADO))) MONTO_LETRAS,
		dbo.FACTURAS.NUMERO_LOTERIA
FROM   dbo.RECIBOS_COBRADOS INNER JOIN
       dbo.FACTURAS ON dbo.RECIBOS_COBRADOS.ID_FACTURA = dbo.FACTURAS.ID_FACTURA INNER JOIN
       dbo.CLIENTES ON dbo.FACTURAS.REFERENCIA = dbo.CLIENTES.REFERENCIA;
</value>
